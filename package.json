{
  "name": "tutor-ai",
  "version": "1.0.0",
  "description": "AI-powered tutoring platform for K-12 education",
  "main": "dist/server.js",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "dev:admin": "tsx watch src/adminServer.ts",
    "build": "tsc || true",
    "start": "node dist/server.js",
    "start:admin": "node dist/adminServer.js",
    "db:push": "prisma db push",
    "db:seed": "tsx prisma/seed.ts",
    "db:studio": "prisma studio",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "docker:build": "docker compose build --no-cache",
    "docker:up": "docker compose up -d",
    "docker:down": "docker compose down",
    "docker:logs": "docker compose logs -f"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "keywords": [
    "ai",
    "tutoring",
    "education",
    "k-12",
    "openai",
    "voice",
    "visual-learning"
  ],
  "author": "AI Digital Assistants",
  "license": "MIT",
  "dependencies": {
    "@paypal/checkout-server-sdk": "^1.0.3",
    "@prisma/client": "^5.19.0",
    "authorizenet": "^1.0.8",
    "bcrypt": "^5.1.1",
    "braintree": "^3.23.0",
    "chart.js": "^4.4.0",
    "connect-redis": "^7.1.0",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.4.0",
    "ejs": "^3.1.10",
    "express": "^4.19.0",
    "express-session": "^1.18.0",
    "ioredis": "^5.4.0",
    "jsonwebtoken": "^9.0.2",
    "katex": "^0.16.9",
    "mermaid": "^10.6.0",
    "multer": "^1.4.5-lts.1",
    "openai": "^4.52.0",
    "passport": "^0.7.0",
    "square": "^38.0.0",
    "stripe": "^17.4.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-microsoft": "^1.0.0",
    "tesseract.js": "^5.0.0",
    "uuid": "^9.0.1",
    "winston": "^3.14.0",
    "winston-daily-rotate-file": "^5.0.0",
    "ws": "^8.18.0",
    "zod": "^3.23.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/connect-redis": "^0.0.23",
    "@types/cookie-parser": "^1.4.7",
    "@types/express": "^4.17.21",
    "@types/express-session": "^1.18.0",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/multer": "^1.4.11",
    "@types/node": "^22.0.0",
    "@types/passport": "^1.0.16",
    "@types/passport-google-oauth20": "^2.0.14",
    "@types/uuid": "^9.0.8",
    "@types/ws": "^8.5.12",
    "prisma": "^5.19.0",
    "tsx": "^4.19.0",
    "typescript": "^5.6.0"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
